---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION, DONATE_URL, CONTACT_EMAIL, INSTAGRAM_URL } from '../consts';
---

<!doctype html>
<html lang="en">
<head>
<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
<style>
.section { width: 100%; }
.inner {
  max-width: 1100px;
  margin: 0 auto;
  padding: 80px 40px;
}
.inner--narrow { max-width: 760px; }

.bg-white { background: #ffffff; }
.bg-gray  { background: #f5f5f5; }
.bg-navy  { background: #1a2e4a; }

/* ── Typography ────────────────────────────────────── */
.eyebrow {
  display: block;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.75rem;
  font-weight: 700;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: #c9921a;
  margin: 0 0 0.75rem;
}
.section-title {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 2rem;
  font-weight: 700;
  color: #1a2e4a;
  line-height: 1.25;
  margin: 0 0 1rem;
}
.section-title--white { color: #ffffff; }
.body-text {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1rem;
  line-height: 1.7;
  color: #333333;
  margin: 0 0 1rem;
}
.body-text:last-child { margin-bottom: 0; }
.body-text--light {
  color: #d1d5db;
}
.text-link {
  color: #1a2e4a;
  font-weight: 600;
  text-decoration: underline;
  text-underline-offset: 2px;
}
.text-link:hover { color: #c9921a; }

/* ── Buttons ───────────────────────────────────────── */
.btn-gold {
  display: inline-block;
  background: #c9921a;
  color: #ffffff;
  text-decoration: none;
  padding: 0.8rem 2rem;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  font-weight: 700;
  letter-spacing: 0.04em;
  text-transform: uppercase;
  border: none;
  cursor: pointer;
  transition: background 0.15s;
}
.btn-gold:hover { background: #a87a14; color: #ffffff; }
.btn-navy {
  display: inline-block;
  background: #1a2e4a;
  color: #ffffff;
  text-decoration: none;
  padding: 0.8rem 2rem;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  font-weight: 700;
  letter-spacing: 0.04em;
  text-transform: uppercase;
  border: none;
  cursor: pointer;
  transition: background 0.15s;
}
.btn-navy:hover { background: #132238; color: #ffffff; }
.btn-white {
  display: inline-block;
  background: #ffffff;
  color: #1a2e4a;
  text-decoration: none;
  padding: 0.8rem 2rem;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  font-weight: 700;
  letter-spacing: 0.04em;
  text-transform: uppercase;
  border: none;
  cursor: pointer;
  transition: background 0.15s;
}
.btn-white:hover { background: #e9edf2; color: #1a2e4a; }

/* ── HERO ──────────────────────────────────────────── */
.hero {
  background: #1a2e4a;
  padding: 100px 40px 90px;
  text-align: center;
  border-bottom: 4px solid #c9921a;
}
.hero h1 {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 2.75rem;
  font-weight: 700;
  color: #ffffff;
  line-height: 1.2;
  margin: 0 auto 1.25rem;
  max-width: 780px;
}
.hero-sub {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1.1rem;
  color: #d1d5db;
  max-width: 580px;
  margin: 0 auto 2.5rem;
  line-height: 1.7;
}
.hero-location {
  display: block;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.8rem;
  font-weight: 600;
  letter-spacing: 0.08em;
  text-transform: uppercase;
  color: #c9921a;
  margin: 0 0 1.25rem;
}
.hero-ctas {
  display: flex;
  gap: 1rem;
  justify-content: center;
  flex-wrap: wrap;
}

/* ── STEPS ─────────────────────────────────────────── */
.steps-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 0;
  margin-top: 3rem;
  border-top: 3px solid #1a2e4a;
}
.step-card {
  padding: 2rem 1.75rem;
  border-right: 1px solid #dde1e7;
}
.step-card:last-child { border-right: none; }
.step-tag {
  display: inline-block;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.7rem;
  font-weight: 700;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: #ffffff;
  background: #c9921a;
  padding: 0.2rem 0.6rem;
  margin-bottom: 1rem;
}
.step-title {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1rem;
  font-weight: 700;
  color: #1a2e4a;
  margin: 0 0 0.5rem;
}
.step-desc {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  color: #4b5563;
  line-height: 1.65;
  margin: 0;
}

/* ── GIVE GRID ─────────────────────────────────────── */
.give-banner {
  background: #1a2e4a;
  border-left: 6px solid #c9921a;
  padding: 2rem 2.5rem;
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 2rem;
  flex-wrap: wrap;
  margin-bottom: 2rem;
}
.give-banner h3 {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1.25rem;
  font-weight: 700;
  color: #ffffff;
  margin: 0 0 0.4rem;
}
.give-banner p {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.95rem;
  color: #d1d5db;
  line-height: 1.6;
  margin: 0;
}
.give-grid {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 1.5rem;
}
.give-card {
  background: #ffffff;
  border: 1px solid #dde1e7;
  border-top: 3px solid #1a2e4a;
  padding: 1.75rem;
}
.give-card h3 {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1rem;
  font-weight: 700;
  color: #1a2e4a;
  margin: 0 0 0.5rem;
}
.give-card p, .give-card ul {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.9rem;
  line-height: 1.65;
  color: #333333;
  margin: 0 0 0.6rem;
}
.give-card ul { padding-left: 1.25rem; }
.give-card p:last-child, .give-card ul:last-child { margin-bottom: 0; }
.give-card a { color: #1a2e4a; font-weight: 600; text-decoration: underline; text-underline-offset: 2px; }
.give-card a:hover { color: #c9921a; }

/* ── CALLOUT ───────────────────────────────────────── */
.callout {
  background: #fffbf0;
  border: 1px solid #e8c96a;
  border-left: 5px solid #c9921a;
  padding: 1.5rem 1.75rem;
  margin-top: 1.5rem;
}
.callout p {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.9rem;
  line-height: 1.7;
  color: #333333;
  margin: 0 0 0.5rem;
}
.callout p:last-child { margin: 0; }
.callout a { color: #1a2e4a; font-weight: 600; }

/* ── ROADMAP ───────────────────────────────────────── */
.roadmap { list-style: none; padding: 0; margin: 2rem 0 0; }
.roadmap-item {
  display: flex;
  gap: 1.5rem;
  align-items: flex-start;
  padding: 1.5rem 0;
  border-bottom: 1px solid #dde1e7;
}
.roadmap-item:last-child { border-bottom: none; }
.roadmap-badge {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.65rem;
  font-weight: 700;
  letter-spacing: 0.08em;
  text-transform: uppercase;
  color: #ffffff;
  padding: 0.25rem 0.6rem;
  white-space: nowrap;
  flex-shrink: 0;
  margin-top: 0.2rem;
}
.roadmap-badge--next   { background: #c9921a; }
.roadmap-badge--filing { background: #1a2e4a; }
.roadmap-badge--planned { background: #4b5563; }
.roadmap-badge--horizon { background: #6b7280; }
.roadmap-content strong {
  font-family: "Inter", system-ui, sans-serif;
  display: block;
  font-size: 0.95rem;
  font-weight: 700;
  color: #1a2e4a;
  margin-bottom: 0.3rem;
}
.roadmap-content p {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  color: #333333;
  line-height: 1.65;
  margin: 0;
}

/* ── FOUNDER ───────────────────────────────────────── */
.founder-block {
  border-left: 5px solid #c9921a;
  padding: 2rem 2rem 2rem 2.25rem;
  background: #f5f5f5;
  margin-top: 2rem;
}
.founder-name {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1.2rem;
  font-weight: 700;
  color: #1a2e4a;
  margin: 0 0 0.2rem;
}
.founder-role {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.75rem;
  font-weight: 600;
  letter-spacing: 0.07em;
  text-transform: uppercase;
  color: #c9921a;
  margin: 0 0 1rem;
}

/* ── STAY CONNECTED ────────────────────────────────── */
.connect-grid {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 1.5rem;
  margin-top: 2rem;
}
.connect-card {
  background: #ffffff;
  border: 1px solid #d1d5db;
  border-top: 3px solid #c9921a;
  padding: 1.75rem;
}
.connect-card h3 {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1rem;
  font-weight: 700;
  color: #1a2e4a;
  margin: 0 0 0.5rem;
}
.connect-card p {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.9rem;
  color: #333333;
  line-height: 1.65;
  margin: 0;
}
.connect-card a { color: #1a2e4a; font-weight: 600; text-decoration: underline; text-underline-offset: 2px; }
.connect-card a:hover { color: #c9921a; }

/* ── RESPONSIVE ────────────────────────────────────── */
@media (max-width: 900px) {
  .steps-grid { grid-template-columns: repeat(2, 1fr); }
  .step-card:nth-child(2) { border-right: none; }
  .step-card:nth-child(3), .step-card:nth-child(4) { border-top: 1px solid #dde1e7; }
}
@media (max-width: 640px) {
  .inner { padding: 56px 20px; }
  .hero { padding: 70px 20px 60px; }
  .hero h1 { font-size: 2rem; }
  .steps-grid { grid-template-columns: 1fr; }
  .step-card { border-right: none; border-bottom: 1px solid #dde1e7; }
  .step-card:first-child { border-top: none; }
  .step-card:last-child { border-bottom: none; }
  .give-grid { grid-template-columns: 1fr; }
  .connect-grid { grid-template-columns: 1fr; }
  .hero-ctas { flex-direction: column; align-items: center; }
  .give-banner { flex-direction: column; }
  .section-title { font-size: 1.6rem; }
  .programs-grid { grid-template-columns: 1fr; }
}

/* ── PROGRAMS PREVIEW ──────────────────────────────── */
.programs-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 1.5rem;
  margin-top: 2.5rem;
}
.program-card {
  border: 1px solid #dde1e7;
  border-top: 3px solid #c9921a;
  padding: 1.75rem;
  display: flex;
  flex-direction: column;
}
.program-card h3 {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1rem;
  font-weight: 700;
  color: #1a2e4a;
  margin: 0 0 0.5rem;
}
.program-card p {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.875rem;
  line-height: 1.65;
  color: #444444;
  margin: 0 0 1.25rem;
  flex: 1;
}
.program-link {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.8rem;
  font-weight: 700;
  color: #c9921a;
  text-decoration: underline;
  text-underline-offset: 2px;
  align-self: flex-start;
}
.program-link:hover { color: #a87a14; }

/* ── TESTIMONIAL ───────────────────────────────────── */
.testimonial-band {
  background: #1a2e4a;
  padding: 72px 40px;
}
.testimonial-inner {
  max-width: 680px;
  margin: 0 auto;
  text-align: center;
}
.testimonial-mark {
  font-family: Georgia, "Times New Roman", serif;
  font-size: 5rem;
  line-height: 0.6;
  color: #c9921a;
  display: block;
  margin-bottom: 1.5rem;
}
.testimonial-text {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 1.2rem;
  font-style: italic;
  line-height: 1.8;
  color: #ffffff;
  margin: 0 0 1.75rem;
}
.testimonial-source {
  font-family: "Inter", system-ui, sans-serif;
  font-size: 0.75rem;
  font-weight: 700;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: rgba(201,146,26,0.9);
}
.testimonial-source span {
  color: rgba(255,255,255,0.5);
  font-weight: 400;
  margin-left: 0.5rem;
}
</style>
</head>
<body>
<Header />
<main id="main-content" aria-label="Main content">

<!-- HERO -->
<section class="hero" aria-label="Hero">
  <span class="hero-location">Pierce &amp; Ware Counties, Georgia</span>
  <h1>Every Community Deserves a Voice. We&rsquo;re Here to Help.</h1>
  <p class="hero-sub">
    Future Voters Academy brings free, nonpartisan civic education directly to low-income neighborhoods &mdash; because knowing how democracy works shouldn&rsquo;t depend on your zip code.
  </p>
  <div class="hero-ctas">
    <a class="btn-gold" href="/donate">Donate Now</a>
    <a class="btn-white" href="/about">Learn More</a>
  </div>
</section>

<!-- ABOUT -->
<section class="section bg-white">
  <div class="inner inner--narrow">
    <span class="eyebrow">Who We Are</span>
    <h2 class="section-title">About Future Voters Academy</h2>
    <p class="body-text">
      Future Voters Academy (FVA) is a community organization working to bring free, nonpartisan civic education to residents in low-income neighborhoods. Everyone deserves the knowledge and tools to participate in our democracy, regardless of their zip code.
    </p>
    <p class="body-text">
      FVA is based in southern Georgia. We are launching our first programs in Pierce and Ware Counties, two rural communities where civic education resources are hard to come by. From there, we plan to grow to more neighborhoods across the region and the country.
    </p>
  </div>
</section>

<!-- MISSION -->
<section class="section bg-gray">
  <div class="inner inner--narrow">
    <span class="eyebrow">Our Purpose</span>
    <h2 class="section-title">Our Mission</h2>
    <p class="body-text">
      Future Voters Academy runs free workshops and creates educational materials to help community members understand their rights, the voting process, and how local, state, and federal government works. Every dollar we receive goes toward workshops, materials, and outreach in communities that need it most.
    </p>
    <p class="body-text">
      Too many people in low-income communities have never had access to real civic education. FVA is here to change that by bringing honest, easy-to-understand civic information directly to the people who need it.
    </p>
  </div>
</section>

<!-- IMPACT NUMBERS removed — to be added once real data is available -->

<!-- HOW IT WORKS -->
<section class="section bg-white">
  <div class="inner">
    <span class="eyebrow">Our Approach</span>
    <h2 class="section-title">What FVA Actually Does</h2>
    <div class="steps-grid">
      <div class="step-card">
        <span class="step-tag">Outreach</span>
        <h3 class="step-title">Finding the Gaps</h3>
        <p class="step-desc">We look for neighborhoods and rural communities where nonpartisan civic education is scarce or has never been offered at all.</p>
      </div>
      <div class="step-card">
        <span class="step-tag">Materials</span>
        <h3 class="step-title">Building the Content</h3>
        <p class="step-desc">We write plain-language guides on voting rights, how government works, and what the electoral process actually looks like from the ground.</p>
      </div>
      <div class="step-card">
        <span class="step-tag">Community</span>
        <h3 class="step-title">Showing Up in Person</h3>
        <p class="step-desc">Free workshops and drop-in sessions bring the information directly to residents — no prior knowledge required, no political agenda.</p>
      </div>
      <div class="step-card">
        <span class="step-tag">Growth</span>
        <h3 class="step-title">Expanding the Reach</h3>
        <p class="step-desc">We grow our donor base and work toward formal nonprofit recognition so the model can spread to more communities over time.</p>
      </div>
    </div>
  </div>
</section>

<!-- PROGRAMS PREVIEW -->
<section class="section bg-gray">
  <div class="inner">
    <span class="eyebrow">What We Offer</span>
    <h2 class="section-title">Our Programs</h2>
    <p class="body-text">Every FVA program is free to attend and built for people with no prior political background. No party affiliation. No agenda. Just the facts.</p>
    <div class="programs-grid">
      <div class="program-card">
        <h3>Voter Education Workshops</h3>
        <p>Free, in-person sessions covering how to register, what&rsquo;s on your ballot, election deadlines, and how to find and contact your elected officials at every level of government.</p>
        <a class="program-link" href="/programs">More about workshops &rarr;</a>
      </div>
      <div class="program-card">
        <h3>Civic Materials Library</h3>
        <p>Plain-language printed guides on voting rights, government structure, and the electoral process. Distributed free to community centers, libraries, food pantries, and housing developments.</p>
        <a class="program-link" href="/programs">More about materials &rarr;</a>
      </div>
      <div class="program-card">
        <h3>Community Outreach</h3>
        <p>We go where residents already are &mdash; partnering with local organizations and civic groups to bring civic education into existing community spaces.</p>
        <a class="program-link" href="/programs">More about outreach &rarr;</a>
      </div>
    </div>
  </div>
</section>

<!-- FOUNDER -->
<section class="section bg-gray">
  <div class="inner inner--narrow">
    <span class="eyebrow">Leadership</span>
    <h2 class="section-title">Meet the Founder</h2>
    <div class="founder-block">
      <p class="founder-name">Thomas Cobb</p>
      <p class="founder-role">Founder &amp; Director, Future Voters Academy</p>
      <p class="body-text">
        Thomas Cobb started Future Voters Academy because he believes civic knowledge is a right, not a privilege. Communities in rural areas like Pierce and Ware Counties deserve the same access to honest, nonpartisan civic education as anyone else. He leads all aspects of FVA's work, from developing educational materials to building the local partnerships needed to deliver them.
      </p>
      <p class="body-text">
        <a class="text-link" href="/about">Learn more about Thomas and FVA &rarr;</a>
      </p>
    </div>
  </div>
</section>

<!-- WAYS TO GIVE -->
<section class="section bg-white">
  <div class="inner">
    <span class="eyebrow">Support FVA</span>
    <h2 class="section-title">Ways to Give</h2>
    <div class="give-banner">
      <div>
        <h3>Support Us on GoFundMe</h3>
        <p>Any amount helps. Your donation goes straight to free civic education for communities that are too often left out of the conversation.</p>
      </div>
      <a class="btn-gold" style="white-space:nowrap; flex-shrink:0;" href="/donate">Donate Now</a>
    </div>
    <div class="give-grid">
      <div class="give-card">
        <h3>One-Time Donation</h3>
        <p>Make a direct contribution in any amount. <a href={`mailto:${CONTACT_EMAIL}`}>Contact us</a> to arrange a donation outside of GoFundMe.</p>
      </div>
      <div class="give-card">
        <h3>Monthly Support</h3>
        <p>Recurring monthly gifts provide reliable funding that allows FVA to plan and expand its programs over time.</p>
      </div>
      <div class="give-card">
        <h3>Organizational Partnership</h3>
        <p>Businesses, nonprofits, and community organizations can partner with FVA through:</p>
        <ul>
          <li><strong>Direct financial contributions</strong> to fund programs</li>
          <li><strong>In-kind support</strong> such as printing, venue space, or supplies</li>
          <li><strong>Co-sponsoring</strong> community workshops and events</li>
        </ul>
        <p><a href={`mailto:${CONTACT_EMAIL}?subject=Partnership Proposal`}>Reach out to start a conversation &rarr;</a></p>
      </div>
      <div class="give-card">
        <h3>Volunteer</h3>
        <p>Contribute your time and skills. We welcome educators, community organizers, and civic-minded professionals. <a href={`mailto:${CONTACT_EMAIL}?subject=Volunteer Interest`}>Get in touch &rarr;</a></p>
      </div>
    </div>
  </div>
</section>

<!-- NONPROFIT STATUS -->
<section class="section bg-gray">
  <div class="inner inner--narrow">
    <span class="eyebrow">Transparency</span>
    <h2 class="section-title">Nonprofit Status</h2>
    <p class="body-text">
      FVA currently operates as an unincorporated association. We are working toward 501(c)(3) nonprofit status. This status will allow us to apply for grants and accept tax-deductible donations. It will also help us build a stronger, more formal organizational structure for the long term.
    </p>
    <div class="callout">
      <p>
        <strong>Please note:</strong> FVA does not yet have 501(c)(3) status, so donations are not tax-deductible at this time. We are grateful for your support while we work through the registration process.
      </p>
      <p>
        Financial records and organizational documents are available upon written request at{' '}
        <a href={`mailto:${CONTACT_EMAIL}`}>{CONTACT_EMAIL}</a>.
      </p>
    </div>
  </div>
</section>

<!-- ROADMAP -->
<section class="section bg-white">
  <div class="inner inner--narrow">
    <span class="eyebrow">Where We Are</span>
    <h2 class="section-title">Near-Term Priorities</h2>
    <p class="body-text">FVA is early-stage and moving deliberately. These are the specific things we are working on right now and building toward:</p>
    <ul class="roadmap">
      <li class="roadmap-item">
        <div class="roadmap-badge roadmap-badge--next">Up Next</div>
        <div class="roadmap-content">
          <strong>First Community Workshop</strong>
          <p>FVA's opening civic education session in Pierce or Ware County. Free to attend, covering voter registration, the electoral process, and how to contact elected officials at every level.</p>
        </div>
      </li>
      <li class="roadmap-item">
        <div class="roadmap-badge roadmap-badge--filing">Filing Now</div>
        <div class="roadmap-content">
          <strong>501(c)(3) Nonprofit Status</strong>
